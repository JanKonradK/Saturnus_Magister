# Environment
ENVIRONMENT=docker

# Database
DATABASE_URL=postgresql://jobtracker:changeme@postgres:5432/jobtracker
POSTGRES_USER=jobtracker
POSTGRES_PASSWORD=changeme
POSTGRES_DB=jobtracker

# xAI / Grok
XAI_API_KEY=xai-your-key-here

# TickTick OAuth
TICKTICK_ACCESS_TOKEN=
TICKTICK_CLIENT_ID=
TICKTICK_CLIENT_SECRET=

# TickTick Project IDs
TICKTICK_Q1_PROJECT=
TICKTICK_Q2_PROJECT=
TICKTICK_Q3_PROJECT=
TICKTICK_Q4_PROJECT=
